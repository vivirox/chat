{
  "name": "open-webui",
  "version": "0.3.32",
  "private": true,
  "scripts": {
    "dev": "npm run pyodide:fetch && vite dev --host",
    "build": "npm run pyodide:fetch && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "npm run lint:frontend ; npm run lint:types ; npm run lint:backend",
    "lint:frontend": "eslint . --fix",
    "lint:types": "npm run check",
    "lint:backend": "pylint backend/",
    "format": "prettier --plugin-search-dir --write \"**/*.{js,ts,svelte,css,md,html,json}\"",
    "format:backend": "black . --exclude \".venv/|/venv/\"",
    "i18n:parse": "i18next --config i18next-parser.config.ts && prettier --write \"src/lib/i18n/**/*.{js,json}\"",
    "cy:open": "cypress open",
    "test:frontend": "vitest --passWithNoTests",
    "pyodide:fetch": "node scripts/prepare-pyodide.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.2.2",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.7.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/typography": "^0.5.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-svelte": "^2.44.1",
    "i18next-parser": "^9.0.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-confetti": "^1.4.0",
    "tailwindcss": "^3.4.13",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@pyscript/core": "^0.4.56",
    "@sveltejs/adapter-node": "^2.1.2",
    "@xyflow/svelte": "^0.1.21",
    "async": "^3.2.6",
    "bits-ui": "^0.19.7",
    "codemirror": "^6.0.1",
    "crc-32": "^1.2.2",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.7",
    "eventsource-parser": "^1.1.2",
    "file-saver": "^2.0.5",
    "fuse.js": "^7.0.0",
    "highlight.js": "^11.10.0",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "idb": "^7.1.1",
    "js-sha256": "^0.10.1",
    "katex": "^0.16.11",
    "marked": "^9.1.6",
    "mermaid": "^10.9.2",
    "newrelic": "^12.5.1",
    "paneforge": "^0.0.6",
    "panzoom": "^9.4.3",
    "pyodide": "^0.26.2",
    "socket.io-client": "^4.8.0",
    "sortablejs": "^1.15.3",
    "svelte-sonner": "^0.3.28",
    "tippy.js": "^6.3.7",
    "turndown": "^7.2.0",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=18.13.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "trustedDependencies": [
    "@contrast/fn-inspect",
    "@newrelic/native-metrics",
    "@sveltejs/kit",
    "cypress",
    "esbuild",
    "protobufjs",
    "svelte-preprocess"
  ]
}
